{
	"breadcrumbs.enabled": false,
	"chat.commandCenter.enabled": false,
	"editor.autoClosingBrackets": "never",
	"editor.autoClosingComments": "never",
	"editor.autoClosingDelete": "never",
	"editor.autoClosingQuotes": "never",
	"editor.autoClosingOvertype": "never",
	"editor.autoIndent": "none",
	"editor.autoSurround": "never",
	"editor.minimap.enabled": false,
	"editor.suggest.selectionMode": "never",
	"editor.wordWrap": "on",
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"files.exclude": {
		"**/.DS_Store": false,
		"**/.git": false,
		"**/.hg": false,
		"**/.svn": false,
		"**/Thumbs.db": false
	},
	"vscode-neovim.neovimExecutablePaths.linux": "/opt/nvim-linux-x86_64/bin/nvim",
	"vscode-neovim.useWSL": true,
	"window.autoDetectColorScheme": true,
	"window.confirmBeforeClose": "always",
	"window.restoreWindows": "none",
	"workbench.editor.wrapTabs": true,
	"workbench.preferredDarkColorTheme": "Gruvbox Dark Soft",
	"workbench.preferredLightColorTheme": "Gruvbox Light Soft",
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "none",
	"workbench.welcomePage.walkthroughs.openOnInstall": false
}
